                  $ref: '#/components/schemas/TarifaConvenioDTO'
                dadosCadastraisConvenio:
                  $ref: '#/components/schemas/DadosCadastraisConvenioDTO'
                assinaturaParteConvenio:
                  $ref: '#/components/schemas/AssinaturaParteConvenioDTO'
            examples:
              Convênio Tarifa Balcão:
                $ref: /convenio_pixautomatico.json#/convenioBalcao
              Convênio Tarifa Diferenciada:
                $ref: /convenio_pixautomatico.json#/convenioDiferenciada
              Convênio Tarifa Campanha:
                $ref: /convenio_pixautomatico.json#/convenioCampanha
        required: true
      responses:
        "400":
          description: Requisição recebida não é válida
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "401":
          description: Não autenticado
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "403":
          description: Token inválido
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "404":
          description: Requisição não retornou dados
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "500":
          description: No momento não foi possível processar sua requisição
        "503":
          description: No momento não foi possível processar sua requisição
        "201":
          description: Requisição processada com sucesso
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Convenio'
      security:
      - SSO TOKEN: []
      - LOGIN: []
      - LOGIN: []
