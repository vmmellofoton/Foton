components:
  schemas:
    Alcada:
      required:
      - nomeAlcada
      type: object
      properties:
        id:
          format: int32
          type: integer
        nivel:
          format: int32
          description: Numero do nivel de alcada
          type: integer
          example: 1
        nomeAlcada:
          description: Nome da alçada
          pattern: \S
          type: string
          example: Agencia
        alcadaAtiva:
          description: Identifica se a alçada está ATIVA ou INATIVA
          type: string
          allOf:
          - $ref: '#/components/schemas/EnumTipoFlag'
          example: S
    AssinantesConvenio:
      type: object
      properties:
        nome:
          type: string
        cpf:
          type: string
        email:
          type: string
        telefone:
          type: string
        tipoAssinante:
          $ref: '#/components/schemas/EnumTipoAssinante'
        vinculo:
          type: string
        matricula:
          type: string
        assinanteComRestricao:
          type: boolean
    AssinaturaParteConvenioDTO:
      type: object
      properties:
        tipoAssinatura:
          $ref: '#/components/schemas/EnumTipoAssinatura'
        socioRepresentanteLegalConvenio:
          type: array
          items:
            $ref: '#/components/schemas/SocioRepresentanteLegalConvenioDTO'
        representanteCaixaConvenio:
          type: array
          items:
            $ref: '#/components/schemas/RepresentanteCaixaConvenioDTO'
        testemunhaConvenio:
          type: array
          items:
            $ref: '#/components/schemas/TestemunhaConvenioDTO'
    AuditoriaDTO:
      required:
      - sessao
      - evento
      - modulo
      - tipoOperacao
      - responsavel
      - datahora
      - enderecoIp
      type: object
      properties:
        sessao:
          description: Identificador da Transacao
          pattern: \S
          type: string
          example: d49eb6f1-0cd2-4e9a-abbd-3474f3da51cd
        evento:
          description: Evento auditável no padrão
          pattern: \S
          type: string
          example: Procedimento X com conteudo Y para finalidade Z
        modulo:
          description: Nome do modulo
          pattern: \S
          type: string
          example: SIACC-PixAutomatico-Template
        tipoOperacao:
          description: Nome da operação
          type: string
          allOf:
          - $ref: '#/components/schemas/EnumTipoOperacao'
        responsavel:
          description: Matricula do responsavel ou nome do processo batch
          pattern: \S
          type: string
          example: c123456
        datahora:
          description: Data/hora do Evento no formato YYYY-MM-DD HH:MM:SS.ZZZZZZ
          pattern: \S
          type: string
          example: '%DATAHORA%'
        enderecoIp:
          description: Endereço IP
          pattern: \S
          type: string
          example: 192.168.0.2
    CampanhaDTO:
      type: object
      properties:
        id:
          format: int32
          type: integer
        nome:
          type: string
        periodo:
          type: string
        vantagens:
          type: string
        publicoAlvo:
          type: string
        descricao:
          type: string
    ChavePix:
      type: object
      properties:
        tipoChave:
          type: string
        agencia:
          type: string
        cnpj:
          type: string
        razaoSocial:
          type: string
        produto:
          type: string
        numeroConta:
          type: string
        dbConta:
          type: string
        quantidadeChavesAtivasVinculadas:
          format: int32
          type: integer
        tipoConta:
          type: string
        chave:
          type: string
        dataInativacao:
          $ref: '#/components/schemas/LocalDateTime'
    ClienteDTOv2:
      type: object
      properties:
        cnpj:
          type: string
        razaoSocial:
          type: string
        restricoesSicow:
          type: array
          items:
            type: string
        necessarioContaPj:
          type: boolean
        cadastroAtualizado:
          type: boolean
        clienteFidelizado:
          type: boolean
        chavePixCadastrada:
          type: boolean
        nomeUnidade:
          type: string
        convenios:
          type: array
          items:
            $ref: '#/components/schemas/ConvenioResponseDTO'
    ContaConvenio:
      required:
      - agencia
      - operacao
      - conta
      - dv
      - tipoConta
      type: object
      properties:
        agencia:
          title: Numero da agência
          description: Número da agênica da conta
          type: integer
        operacao:
          title: Número da operação da conta
          description: Número da operação da conta
          type: integer
        conta:
          format: int64
          title: Número da conta
          description: Número da conta CAIXA para repasse de valores para o conveniado.
          type: integer
        dv:
          title: Dígito da conta
          description: Dígito da conta CAIXA para repasse de valores para o conveniado.
          type: integer
        tipoConta:
          title: Tipo da conta
          description: Indica o a finalidade da conta. Conta para repasse/debitar
            tarifa
          type: string
          allOf:
          - $ref: '#/components/schemas/EnumTipoConta'
    ContaDTO:
      type: object
      properties:
        numeroAgencia:
          format: int32
          type: integer
        codigoOperacao:
          format: int32
          type: integer
        numeroConta:
          format: int64
          type: integer
        dvConta:
          format: int32
          type: integer
    ContaPj:
      type: object
      properties:
        id:
          format: int64
          type: integer
        codigoOperacao:
          type: string
        numeroAgencia:
          type: string
        numeroConta:
          type: string
        dbConta:
          type: string
    ContatoConvenioDTO:
      type: object
      properties:
        email:
          type: string
        telefone:
          type: array
          items:
            type: string
    Convenio:
      required:
      - id
      - tipoServico
      - cnpj
      - convenioInterno
      - unidadeInclusao
      - unidadeRelacionamento
      - convenioOriginal
      - tipoAssinatura
      - listaSituacao
      type: object
      properties:
        id:
          format: int64
          title: Identificador do convênio negocial
          description: Preenchido com numero único de identificação do convênio
          type: integer
        simulacaoNegocialConvenio:
          title: Simulação usada para gerar o convênio
          type: object
          allOf:
          - $ref: '#/components/schemas/SimulacaoNegocialConvenio'
        tipoServico:
          title: Tipo de serviço
          description: Identifica o tipo de serviço prestado
          type: object
          allOf:
          - $ref: '#/components/schemas/TipoServico'
        cnpj:
          title: CNPJ do solicitante
