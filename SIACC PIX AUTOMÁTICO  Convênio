  /convenio:
    post:
      tags:
      - API Convênio
      summary: Inclusão
      description: Efetua a inclusão da Convênio
      requestBody:
        description: Selecione o arquivo a ser enviado
        content:
          application/json:
            schema:
              type: object
              properties:
                apelidoConvenio:
                  type: string
                unidadeRelacionamento:
                  format: int32
                  type: integer
