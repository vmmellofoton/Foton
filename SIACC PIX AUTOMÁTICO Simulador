      - SSO TOKEN: []
      - LOGIN: []
      - LOGIN: []
  /simulacao/alcada:
    post:
      tags:
      - API Simulação Alçada
      summary: Alteração
      description: aprovar simulação por alçada
      requestBody:
        description: Simulação com parecer por alçada
        content:
          multipart/form-data:
            schema:
              required:
              - simulacaoAlcada.numeroSimulacaoNegocialConvenio
              - simulacaoAlcada.aprovada
              type: object
              properties:
                simulacaoAlcada.numeroSimulacaoNegocialConvenio:
                  format: int32
                  title: Numero da Simulação Negocial do Convenio
                  description: Numero da Simulação Negocial do Convenio
                  type: integer
                simulacaoAlcada.aprovada:
                  title: Tipo da Situação
                  description: True - (4 - Simulação Aprovada) / False - (7 - Simulação
                    Reprovada)
                  type: boolean
                  example: True - (4 - Simulação Aprovada) / False - (7 - Simulação
                    Reprovada)
                simulacaoAlcada.parecer:
                  title: Justificativa das Simulação negocial
                  description: Justificativa
                  type: string
                simulacaoAlcada.documentos:
                  type: array
                  items:
                    $ref: '#/components/schemas/UploadItemSchema'
        required: true
      responses:
        "400":
          description: Requisição recebida não é válida
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "401":
          description: Não autenticado
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "403":
          description: Token inválido
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "404":
          description: Requisição não retornou dados
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
