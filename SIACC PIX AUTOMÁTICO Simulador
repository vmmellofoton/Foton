  /simulacao/consultaAvancada:
    get:
      tags:
      - API Simulação
      summary: Consulta Avançada
      description: Consultar simulações cadastradas
      parameters:
      - name: cnpj
        in: query
        description: "Filtro pelo número de cnpj. Ex: 40191968000167"
        schema:
          type: string
      - name: dataAprovacaoAte
        in: query
        description: "Filtro até uma data aprovação da simulação. Ex: 2024-05-21"
        examples:
          VAZIO: {}
          VALOR INVALIDO:
            value: ISTO NAO É UMA DATA
          FORMATO INVALIDO:
            value: 21-05-2024
        content:
          '*/*':
            schema:
              default: ""
      - name: dataAprovacaoDe
        in: query
        description: "Filtro a partir de uma data aprovação da simulação. Ex: 2024-05-21"
        examples:
          VAZIO: {}
          VALOR INVALIDO:
            value: ISTO NAO É UMA DATA
          FORMATO INVALIDO:
            value: 21-05-2024
        content:
          '*/*':
            schema:
              default: ""
      - name: dataInclusaoAte
        in: query
        description: "Filtro até uma data de inclusão da simulação. Ex: 2024-05-21"
        examples:
          VAZIO: {}
          VALOR INVALIDO:
            value: ISTO NAO É UMA DATA
          FORMATO INVALIDO:
            value: 21-05-2024
        content:
          '*/*':
            schema:
              default: ""
      - name: dataInclusaoDe
        in: query
        description: "Filtro a partir de uma data de inclusão da simulação. Ex: 2024-05-21"
        examples:
          VAZIO: {}
          VALOR INVALIDO:
            value: ISTO NAO É UMA DATA
          FORMATO INVALIDO:
            value: 21-05-2024
        content:
          '*/*':
            schema:
              default: ""
      - name: numeroSimulacao
        in: query
        description: "Filtro pelo número de simulação. Ex: 1"
        schema:
          format: int64
          type: integer
      - name: page
        in: query
        description: "Número da página. Ex: 0"
        required: true
        schema:
          format: int32
          type: integer
        example: 0
      - name: size
        in: query
        description: "Quantidade de itens por página (máx de 10). Ex: 10"
        required: true
        schema:
          format: int32
          maximum: 60
          type: integer
        example: 10
      - name: tipoSituacaoId
        in: query
        description: "Filtro pelo id do tipo situação da simulação. Ex: 1, 2, 3..."
        schema:
          type: string
      - name: unidadeRelacionamento
        in: query
        description: "Filtro pela unidade de relacionamento. Ex: 1520"
        schema:
          type: integer
      responses:
        "400":
          description: Requisição recebida não é válida
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "401":
          description: Não autenticado
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
