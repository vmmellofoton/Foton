  /simulacao:
    post:
      tags:
      - API Simulação
      summary: Inclusão
      description: Efetua a inclusão da Simulação
      requestBody:
        description: Selecione o arquivo a ser enviado
        content:
          multipart/form-data:
            schema:
              required:
              - condicaoTarifaPagadoresCaixa
              - condicaoTarifaPagadoresOutras
              - condicaoTarifaRetentativa
              - condicaoDiasDegustacao
              type: object
              properties:
                cnpj:
                  type: string
                condicaoTipo:
                  format: int32
                  type: integer
                condicaoId:
                  format: int32
                  type: integer
                condicaoNome:
                  type: string
                condicaoTarifaPagadoresCaixa:
                  format: double
                  minimum: 0
                  type: number
                condicaoTarifaPagadoresOutras:
                  format: double
                  minimum: 0
                  type: number
                condicaoTarifaRetentativa:
                  format: double
                  minimum: 0
                  type: number
                condicaoDiasDegustacao:
                  format: int32
                  minimum: 0
                  type: integer
                excecaoContaDebitoDiferente:
                  type: boolean
                excecaoInibirRetentativa:
                  type: boolean
                excecaoInibirOpenFinance:
                  type: boolean
                excecaoMinutaContratualForaPadrao:
                  type: boolean
                minutaEditalCredenciamento:
                  type: boolean
                excecaoOutro:
                  type: boolean
                excecaoTextoOutro:
                  type: string
                justificativaNovoConvenio:
                  type: boolean
                justificativaUnidadeRelacionamentoConvenio:
                  type: string
                justificativaAgendamentosPorMes:
                  format: int32
                  type: integer
                justificativaTicketMedio:
                  format: double
                  type: number
                justificativaMigracaoDebitoAutomatico:
                  type: boolean
                justificativaContaPJDebitoAutomatico:
                  type: string
                justificativaComentarios:
                  type: string
                justificativaDocumentos:
                  type: array
                  items:
                    $ref: '#/components/schemas/UploadItemSchema2'
            examples:
              Simulação Tarifa Balcão:
                $ref: /simulacao_pixautomatico.json#/simulacaoBalcao
              Simulação Tarifa Diferenciada:
                $ref: /simulacao_pixautomatico.json#/simulacaoDiferenciada
              Simulação Tarifa Campanha:
                $ref: /simulacao_pixautomatico.json#/simulacaoCampanha
        required: true
      responses:
        "400":
          description: Requisição recebida não é válida
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "401":
          description: Não autenticado
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "403":
          description: Token inválido
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "404":
          description: Requisição não retornou dados
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/ResponseDTO'
        "500":
          description: No momento não foi possível processar sua requisição
        "503":
          description: No momento não foi possível processar sua requisição
        "201":
          description: Requisição processada com sucesso
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/SimulacaoNegocialConvenio'
